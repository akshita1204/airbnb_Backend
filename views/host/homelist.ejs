
    <%- include('../partials/head') %>
    <link href="/public/output.css" rel="stylesheet">
</head>
<body>
   <header><h1>Registered Successfully!</h1></header> 
   <ul class="space-y-6">
    <% registered.forEach(home => { %>
      <li class="bg-green-50 border border-green-200 rounded-lg shadow-lg p-6">
        <h3 class="text-xl font-semibold text-gray-800 mb-2"><%= home.hname %></h3>
        <div class="text-sm text-gray-600">
          <strong>Rating:</strong> <%= home.rating %>
        </div>
        <div class="text-sm text-gray-600">
          <strong>Price:</strong> <%= home.price %>
        </div>
        <div class="text-sm text-gray-600">
          <strong>Location:</strong> <%= home.location %>
        </div>
        <button class="mt-4 bg-green-500 text-white px-6 py-2 rounded hover:bg-green-400 transition duration-300">EDIT</button>
        <button class="mt-4 bg-green-500 text-white px-6 py-2 rounded hover:bg-green-400 transition duration-300">DELETE</button>
      </li>
    <% }) %>
  </ul>
</body>
</html>